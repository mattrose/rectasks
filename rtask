#!/usr/bin/env bash
# vim: set ts=4 sw=4 et:
case $1 in 
    add)
        shift 1 
        bash rtask-add "$@"
        ;;
    list)
        shift 1 
        bash rtask-list "$@"
        ;;
    comment)
        shift 1 
        bash rtask-comment "$@"
        ;;
    *)
        echo "no"
        exit 1
        ;;
esac    
